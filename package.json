{
  "name": "generator-api",
  "version": "1.5.3",
  "description": "Yeoman generator for creating RESTful NodeJS APIs, using ES6, Mongoose and Express",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm test",
    "test": "mocha --bail test/*.spec.js",
    "lint": "standard",
    "release": "np"
  },
  "author": "ndelvalle <nicolas.delvalle@gmail.com>",
  "contributors": [
    "Christian Gill <gillchristiang@gmail.com> (https://gillchristian.xyz/)"
  ],
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "generator-api",
    "api",
    "mongoose",
    "mongo",
    "express"
  ],
  "files": [
    "generators/app",
    "generators/model"
  ],
  "dependencies": {
    "esprima": "^4.0.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.12.1",
    "to-case": "^2.0.0",
    "yeoman-generator": "^2.0.3",
    "yosay": "^2.0.0"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.3.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.17.3",
    "np": "^2.12.0",
    "pre-commit": "^1.1.3",
    "standard": "^11.0.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "bugs": {
    "url": "https://github.com/ndelvalle/generator-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndelvalle/generator-api.git"
  },
  "pre-commit": [
    //"lint"
  ],
  "standard": {
